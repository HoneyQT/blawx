    scasp_encoding: 'blawx_category(test_result).

      #pred test_result(X) :: ''@(X) is a test_result''.

      #pred according_to(R,test_result(X)) :: ''according to @(R), @(X) is a test_result''.

      #pred legally_holds(_,test_result(X)) :: ''it legally holds that @(X) is a test_result''.

      test_result(negative).

      test_result(positive).

      blawx_category(person).

      #pred person(X) :: ''@(X) is a person''.

      #pred according_to(R,person(X)) :: ''according to @(R), @(X) is a person''.

      #pred legally_holds(_,person(X)) :: ''it legally holds that @(X) is a person''.

      blawx_category(test).

      #pred test(X) :: ''@(X) is a test''.

      #pred according_to(R,test(X)) :: ''according to @(R), @(X) is a test''.

      #pred legally_holds(_,test(X)) :: ''it legally holds that @(X) is a test''.

      blawx_attribute(test,taken_at,datetime).

      blawx_attribute_nlg(taken_at,ov,"test","was taken at","").

      #pred taken_at(X,Y) :: ''test @(X) was taken at @(Y)''.

      #pred according_to(R,taken_at(X,Y)) :: ''according to @(R), test @(X) was taken
      at @(Y)''.

      #pred legally_holds(_,taken_at(X,Y)) :: ''it legally holds that test @(X) was
      taken at @(Y)''.


      blawx_attribute(test,result,test_result).

      blawx_attribute_nlg(result,ov,"the result of test","was","").

      #pred result(X,Y) :: ''the result of test @(X) was @(Y)''.

      #pred according_to(R,result(X,Y)) :: ''according to @(R), the result of test
      @(X) was @(Y)''.

      #pred legally_holds(_,result(X,Y)) :: ''it legally holds that the result of
      test @(X) was @(Y)''.


      blawx_attribute(test,person_tested,person).

      blawx_attribute_nlg(person_tested,ov,"test","is with regard to person","").

      #pred person_tested(X,Y) :: ''test @(X) is with regard to person @(Y)''.

      #pred according_to(R,person_tested(X,Y)) :: ''according to @(R), test @(X) is
      with regard to person @(Y)''.

      #pred legally_holds(_,person_tested(X,Y)) :: ''it legally holds that test @(X)
      is with regard to person @(Y)''.


      blawx_category(flight).

      #pred flight(X) :: ''@(X) is a flight''.

      #pred according_to(R,flight(X)) :: ''according to @(R), @(X) is a flight''.

      #pred legally_holds(_,flight(X)) :: ''it legally holds that @(X) is a flight''.

      blawx_attribute(flight,departure_time,datetime).

      blawx_attribute_nlg(departure_time,ov,"","departs at","").

      #pred departure_time(X,Y) :: ''@(X) departs at @(Y)''.

      #pred according_to(R,departure_time(X,Y)) :: ''according to @(R), @(X) departs
      at @(Y)''.

      #pred legally_holds(_,departure_time(X,Y)) :: ''it legally holds that @(X) departs
      at @(Y)''.


      blawx_attribute(flight,ticket_holder,person).

      blawx_attribute_nlg(ticket_holder,vo,"","is ticketed to fly on ","").

      #pred ticket_holder(Y,X) :: ''@(X) is ticketed to fly on  @(Y)''.

      #pred according_to(R,ticket_holder(Y,X)) :: ''according to @(R), @(X) is ticketed
      to fly on  @(Y)''.

      #pred legally_holds(_,ticket_holder(Y,X)) :: ''it legally holds that @(X) is
      ticketed to fly on  @(Y)''.


      blawx_attribute(person,permitted_to_take_flight,flight).

      blawx_attribute_nlg(permitted_to_take_flight,ov,"","is permitted to fly on","").

      #pred permitted_to_take_flight(X,Y) :: ''@(X) is permitted to fly on @(Y)''.

      #pred according_to(R,permitted_to_take_flight(X,Y)) :: ''according to @(R),
      @(X) is permitted to fly on @(Y)''.

      #pred legally_holds(_,permitted_to_take_flight(X,Y)) :: ''it legally holds that
      @(X) is permitted to fly on @(Y)''.



      permitted_to_take_flight(Person,Flight) :-

      person(Person),

      flight(Flight),

      test(Test),

      ticket_holder(Flight,Person),

      person_tested(Test,Person),

      departure_time(Flight,Departure),

      result(Test,negative),

      taken_at(Test,TestTime),

      datetime_add(TestTime,duration(1,0,0,0,72,0,0),LatestDeparture),

      not_after(Departure,LatestDeparture).'
